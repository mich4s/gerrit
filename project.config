[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/mich4s
[access "refs/for/refs/*"]
	read = group user/mich4s
	push = group user/mich4s
	label-Code-Review = -2..+2 group user/mich4s
	label-Verified = -1..+1 group user/mich4s
	rebase = group user/mich4s
	removeReviewer = group user/mich4s
	submit = group user/mich4s
[access "refs/heads/*"]
	read = group user/mich4s
	create = group user/mich4s
	push = +force group user/mich4s
	pushMerge = group user/mich4s
[access "refs/tags/*"]
	push = +force group user/mich4s
	pushTag = group user/mich4s
	pushSignedTag = group user/mich4s
